{
  "name": "nzta/member-bookmark",
  "description": "A module for creating member global bookmarks",
  "type": "silverstripe-vendormodule",
  "homepage": "https://github.com/NZTA/member-bookmark",
  "keywords": [
    "silverstripe",
    "bookmark",
    "favourites",
    "cms"
  ],
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/NZTA/member-bookmark/issues"
  },
  "require": {
    "silverstripe/framework": "~4.1",
    "silverstripe/cms": "~4.1",
    "sheadawson/silverstripe-linkable": "^2.0",
    "undefinedoffset/sortablegridfield": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "NZTA\\MemberBookmark\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NZTA\\MemberBookmark\\Tests\\": "tests/"
    }
  }
}
